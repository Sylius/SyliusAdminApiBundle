{
    "name": "sylius/admin-api-bundle",
    "type": "symfony-bundle",
    "description": "APIs management for Symfony projects.",
    "keywords": [
        "api",
        "rest"
    ],
    "homepage": "https://sylius.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "https://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "https://sylius.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "friendsofsymfony/oauth-server-bundle": ">2.0.0-alpha.0 ^2.0@dev",
        "sylius-labs/doctrine-migrations-extra-bundle": "^0.1.3",
        "sylius-labs/polyfill-symfony-framework-bundle": "^1.0",
        "sylius/sylius": "^1.10 || ^1.11",
        "symfony/form": "^4.4 || ^5.2 || ^6.0",
        "symfony/framework-bundle": "^4.4 || ^5.2 || ^6.0",
        "symfony/validator": "^4.4 || ^5.2 || ^6.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.5",
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "doctrine/orm": "^2.7",
        "lchrusciel/api-test-case": "^5.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpspec/phpspec": "^7.0",
        "phpunit/phpunit": "^9.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/browser-kit": "^4.4 || ^5.2 || ^6.0",
        "symfony/debug-bundle": "^4.4 || ^5.2 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.2 || ^6.0",
        "symfony/dotenv": "^4.4 || ^5.2 || ^6.0",
        "symfony/monolog-bundle": "^3.7",
        "symfony/web-profiler-bundle": "^4.4 || ^5.2 || ^6.0",
        "twig/twig": "^2.12",
        "vimeo/psalm": "4.6.4"
    },
    "conflict": {
        "doctrine/dbal": "^3",
        "doctrine/orm": "^2.10",
        "friendsofsymfony/rest-bundle": "^3.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/thanks": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Sylius\\Bundle\\AdminApiBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sylius\\Bundle\\AdminApiBundle\\spec\\": "spec/",
            "Tests\\Sylius\\Bundle\\AdminApiBundle\\": "tests/"
        }
    }
}
